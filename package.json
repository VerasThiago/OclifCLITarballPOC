{
  "name": "tarballcli",
  "version": "0.0.4",
  "author": "Thiago Veras @VerasThiago",
  "bin": {
    "tarballcli": "./bin/run"
  },
  "bugs": "https://github.com/VerasThiago/tarballcli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@thiagoveras/plugin-update": "^0.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "aws-sdk": "^2.741.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/VerasThiago/tarballcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tarballcli",
    "update": {
      "autoupdate": {
        "rollout": 50,
        "debounce": 60
      },
      "node": {
        "version": "12.12.0"
      },
      "s3": {
        "bucket": "vtex-toolbelt-test",
        "xz": true
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@thiagoveras/plugin-update"
    ]
  },
  "repository": "VerasThiago/tarballcli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build-clean": "rm -rf build",
    "build": "yarn build-clean && yarn tsc && yarn oclif-dev manifest"
  },
  "types": "lib/index.d.ts"
}
